{
    "name": "baldcat/orchid-webp",
    "description": "Composer package for converting Orchid Attachment to webp.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Baldcat\\OrchidWebp\\": "src",
            "Baldcat\\OrchidWebp\\Service\\": "src/Services",
            "Baldcat\\OrchidWebp\\Traits\\": "src/Traits",
            "Baldcat\\OrchidWebp\\Storage\\": "src/Storage"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Baldcat\\OrchidWebp\\OrchidWebpServiceProvider"
            ]
        }
    },
    "version": "0.1.0",
    "require": {
        "php": "^8.3",
        "orchid/platform": "^14.0",
      "ext-gd": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^9.5",
        "laravel/pint": "^1.21"
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
